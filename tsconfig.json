{
  "compilerOptions": {
    "incremental": true,
    "module": "commonjs",
    "target": "es2017",
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,        
    "skipLibCheck": true,
    "outDir": "dist",
    "types": [ "react-dom/next" ]
  }
}
